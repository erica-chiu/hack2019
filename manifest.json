{
    "name": "AdBlockerBlockerBlocker",
    "version": "1.0",
    "description": "Blacklist certain websites to block javascript from running",
    "permissions": ["activeTab", "contentSettings", "declarativeContent", "storage"],
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "options_page": "options.html",
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/rubberduck16.jpg",
            "32": "images/rubberduck32.jpg",
            "48": "images/rubberduck48.jpg",
            "128": "images/rubberduck128.jpg"
        }
    },
    "icons": {
        "16": "images/rubberduck16.jpg",
        "32": "images/rubberduck32.jpg",
        "48": "images/rubberduck48.jpg",
        "128": "images/rubberduck128.jpg"
    },
    "manifest_version": 2
}
