{
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": ["activeTab", "declarativeContent", "storage"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "options_page": "options.html",
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/rubberduck16.jpg",
            "32": "images/rubberduck32.jpg",
            "48": "images/rubberduck48.jpg",
            "128": "images/rubberduck128.jpg"
        }
    },
    "icons": {
        "16": "images/rubberduck16.jpg",
        "32": "images/rubberduck32.jpg",
        "48": "images/rubberduck48.jpg",
        "128": "images/rubberduck128.jpg"
    },
    "manifest_version": 2
}
