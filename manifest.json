{
    "name": "AdBlockerBlockerBlocker",
    "version": "1.0",
    "description": "Blacklist certain websites to block javascript from running",
    "permissions": ["activeTab", "contentSettings", "declarativeContent", "storage", "tabs"],
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "options_page": "options.html",
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "300": "images/block.png"
        }
    },
    "icons": {
        "300": "images/block.png"
    },
    "manifest_version": 2
}
